[alias]
        co = checkout
        br = branch
        cob = checkout -b
        brd = branch -D
        rb = rebase
        rba = rebase --abort
        rbc = rebase --continue
        lg = log --oneline
        st = status -sb
        pl = pull --prune
        sh = stash 
        shl = stash list
        shp = stash pop
